{
  "name": "vscode-cellox",
  "displayName": "vscode-cellox",
  "description": "Cellox language support for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "cellox",
      "aliases": ["cellox", "cellox"],
      "extensions": [".clx"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cellox",
      "scopeName": "source.cellox",
      "path": "./syntaxes/cellox.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "cellox",
        "path": "./snippets.json"
      }
    ],
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "esbuild --bundle --sourcemap=external --minify --external:vscode src/extension.ts --outdir=out --platform=node --format=cjs"
    }
  }
}
